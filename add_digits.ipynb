        "class Solution(object):\n",
        "    def addDigits(self, num):\n",
        "        if num <= 9:\n",
        "            return num\n",
        "        else:\n",
        "            numstr = str(num)\n",
        "            temp = 0\n",
        "            for i in numstr:\n",
        "                temp += int(i)\n",
        "            return self.addDigits(temp)\n",
        ""
